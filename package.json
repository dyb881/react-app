{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dyb881/fetch-request": "^0.4.0",
    "@dyb881/router": "^1.1.1",
    "@hot-loader/react-dom": "^16.12.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "antd-dayjs-webpack-plugin": "^0.0.8",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0-alpha.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "react": "^16.13.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.19",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typescript": "~3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "PUBLIC_URL=. react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "start:https": "HTTPS=true yarn start",
    "build:production": "GENERATE_SOURCEMAP=false yarn build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": 0
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true,
    "requirePragma": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
