    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "antd-dayjs-webpack-plugin": "^1.0.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^1.0.0-alpha.0",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "~3.8.3",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "PUBLIC_URL=. react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "start:https": "HTTPS=true yarn start",
    "build:production": "PUBLIC_URL=. GENERATE_SOURCEMAP=false react-app-rewired build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": 0
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true,
    "requirePragma": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
